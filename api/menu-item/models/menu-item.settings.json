{
  "kind": "collectionType",
  "collectionName": "menu_items",
  "info": {
    "name": "Menu Item"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "text": {
      "type": "string",
      "required": true,
      "maxLength": 50
    },
    "is_external_link": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "text",
      "required": true,
      "maxLength": 50
    },
    "order": {
      "type": "integer",
      "required": true,
      "default": 0,
      "min": 0
    },
    "page": {
      "via": "menu_item",
      "model": "page"
    }
  }
}
